{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "rootDir": "",
        "moduleResolution": "node",
        "outDir": "./dist",
        "esModuleInterop": true,
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "strictPropertyInitialization": false,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}